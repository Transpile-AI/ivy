'''
Creating an empty tensor sequence with given data type (default is float)
'''

from onnx import helper


class SequenceEmpty:
    @constructor
    def __init__(self, name, dtype=float, shape):
        self.name = name
        self.dtype = dtype
        self.shape = shape
        self.tensor = helper.make_tensor(self.name, self.dtype, self.shape, 0)
    
    @classmethod
    def export(self):
        return self.tensor
